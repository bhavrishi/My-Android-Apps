<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">


    <ImageView
        android:id="@+id/detpic"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/default_image"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/detpic"
        android:text="First Name" />

    <TextView
        android:id="@+id/detName"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:text="@string/detName"
        android:layout_below="@+id/detpic"
        android:layout_alignStart="@+id/detpic" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView2"
        android:layout_marginTop="16dp"
        android:text="Last Name" />

    <TextView
        android:id="@+id/detLastName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/detName"
        android:layout_alignStart="@+id/detName"
        android:layout_alignTop="@+id/textView4" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView4"
        android:layout_marginTop="22dp"
        android:text="Phone Number"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView6"
        android:layout_marginTop="20dp"
        android:text="Email" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView7"
        android:layout_marginTop="25dp"
        android:text="URL" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView8"
        android:layout_marginTop="22dp"
        android:text="Skype URL" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView9"
        android:layout_marginTop="18dp"
        android:text="Youtube URL" />

    <TextView
        android:id="@+id/detPhoneNumber"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/detLastName"
        android:layout_alignStart="@+id/detLastName"
        android:layout_alignTop="@+id/textView6" />

    <TextView
        android:id="@+id/detEmail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/detPhoneNumber"
        android:layout_alignStart="@+id/detPhoneNumber"
        android:layout_alignTop="@+id/textView7" />

    <TextView
        android:id="@+id/detUrl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/detEmail"
        android:layout_alignStart="@+id/detEmail"
        android:layout_alignTop="@+id/textView8" />

    <TextView
        android:id="@+id/detSkypeUrl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/detUrl"
        android:layout_alignStart="@+id/detUrl"
        android:layout_alignTop="@+id/textView9" />

    <TextView
        android:id="@+id/detYutubeUrl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/detSkypeUrl"
        android:layout_alignStart="@+id/detSkypeUrl"
        android:layout_alignTop="@+id/textView10" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView10"
        android:layout_marginTop="34dp"
        android:text="FB Url"
        android:layout_alignParentStart="true"
        android:layout_alignEnd="@+id/textView6" />

    <TextView
        android:id="@+id/detFbUrl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/detYutubeUrl"
        android:layout_alignStart="@+id/detYutubeUrl"
        android:layout_alignTop="@+id/textView14" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView14"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView14"
        android:text="Twitter" />

    <TextView
        android:id="@+id/detTwitter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView16"
        android:layout_alignEnd="@+id/detFbUrl"
        android:layout_alignStart="@+id/detFbUrl" />

    <TextView
        android:id="@+id/Companys"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView9"
        android:layout_below="@+id/textView16"
        android:layout_marginTop="23dp"
        android:text="Company" />

    <TextView
        android:id="@+id/detCompany"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/Companys"
        android:layout_alignEnd="@+id/detTwitter"
        android:layout_alignStart="@+id/detTwitter" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView16"
        android:layout_alignStart="@+id/Companys"
        android:layout_below="@+id/Companys"
        android:layout_marginStart="6dp"
        android:layout_marginTop="42dp"
        android:text="Address" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView20"
        android:layout_below="@+id/textView20"
        android:layout_marginTop="34dp"
        android:text="Birthday" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView20"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView21"
        android:layout_marginTop="36dp"
        android:text="Nickname" />

    <TextView
        android:id="@+id/detAddress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/detCompany"
        android:layout_alignStart="@+id/detCompany"
        android:layout_alignTop="@+id/textView20"
        android:layout_marginStart="15dp"
        android:layout_marginTop="6dp" />

    <TextView
        android:id="@+id/detBirthday"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView22"
        android:layout_alignEnd="@+id/detAddress"
        android:layout_alignStart="@+id/detAddress" />

    <TextView
        android:id="@+id/detNickname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView22"
        android:layout_alignEnd="@+id/detBirthday"
        android:layout_alignStart="@+id/detCompany"
        android:layout_marginStart="8dp" />

</RelativeLayout>
